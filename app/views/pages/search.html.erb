<h1>Logements disponibles</h1>
<table  >
    <thead>
        <tr>
            <th>Title </th>
            <th>Adresse</th>
            <th>City</th>
            <th>Depart</th>
            <th>Arriv√©e</th>
        </tr>
    </thead>
    <tbody >
        <!--<div data-controller="places" data-places-latlng-value="<%= @latlng %>"></div>-->
        <%  @logements_disponible.each do |logement| %>
            <tr >
                <td><%= logement.title %></td>
                <td><%= logement.adresse %></td>
                <td><%= logement.city %></td>
                <td><%= logement.start_date_of_availability.try(:strftime, ("%e %B %Y")) %></td> 
                <td><%= logement.end_date_of_availability.try(:strftime, ("%e %B %Y")) %></td>
            </tr>
        <% end %> 
    </tbody>
</table>

<%= tag.div id: "map", data: { l: @logements_disponible.to_json(methods: [:address])} %> 

