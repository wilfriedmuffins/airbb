<h1>Utilisateurs du site</h1>

<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>

    <% @users.each do |user|%>
        <tr>
            <td> <%= user.first_name %> </td>
            <td> <%= user.last_name %> </td>
            <td> <%= user.email %> </td>
        </tr>
    <% end %>
    </tbody>
</table>
