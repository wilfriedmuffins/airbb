

    <%= @logement.title %>
    <%= @logement.adresse %>
    <%= @logement.zipcode %>
    <%= @logement.city %>
    <%= @logement.voyageur %>
    <%= @logement.start_date_of_availability.try(:strftime, ("%e %B %Y")) %>
    <%= @logement.end_date_of_availability.try(:strftime, ("%e %B %Y")) %>
    <%= link_to "show", logement_path(@logement) %>
    <% @logement.images.each do |photo| %>
        <%= image_tag photo.url %>
    <% end %>

<%= tag.div id: "map", data: { l: current_user.logements.to_json(methods: [:address])} %> 

<ul>
    <li><%= link_to "Edit", edit_logement_path(@logement) %></li>
    <li><%= link_to "Suprimer", logement_path(@logement), method: :delete %></li>
    <li><%= link_to "Retour", edit_user_registration_path %></li>
</ul> 

